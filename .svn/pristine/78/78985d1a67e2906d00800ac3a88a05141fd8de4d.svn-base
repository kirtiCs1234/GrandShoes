@section scripts{
    <script type="text/javascript">
        $(document).on("click", "#cancelyesbtn", function () {
            window.location = '/admin/SizeGrid/index';
        })
        var id1;
        function create() {
            Data(0);
        }

        function edit(edit) {
            id1 = edit;//$(this).data("id");
            Data(1);
        }

        function details(details) {
            id1 = details;//$(this).data("id");
            Data(2);
        }

        function del(del) {
            id1 = del;//"";
            Data(3);
        }

        //  $(".approve").click(function () {
        //       id2 = $(this).data("id");
        //      approved();
        //  });
        function Data(ch) {
            debugger;
            var crud = "";
            switch (ch) {
                case 0:
                    crud = '/Admin/SizeGrid/Create';
                    break;
                case 1:
                    crud = '/Admin/SizeGrid/Edit/' + id1;
                    break;
                case 2:
                    crud = '/Admin/SizeGrid/Details/' + id1;
                    break;
                case 3:
                    crud = '/Admin/SizeGrid/Delete/' + id1;
                    break;
                
            }
            $.ajax({

                url: crud,
                contentType: 'application/html; charse=utf-8',
                type: 'GET',
                // data: {page : page} ,
                dataType: 'html',
                success: function (result) {

                    $('#dvCreatedetails').html(result);
                },
                error: function (xhr, status) {
                    alert(status)
                }
            });
        }

</script>

    }