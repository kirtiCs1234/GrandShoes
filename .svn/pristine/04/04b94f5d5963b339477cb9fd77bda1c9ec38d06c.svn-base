using Services.IService;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using Model.Product;
using Helper;

namespace Services.Service
{
    class ProductService : IProductService
    {
        public List<Product> ProductAutocomplete(string name)
        {
            return ServerResponse.Invoke<List<Product>>("api/Product/ProductsAutocomplete?name=" + name, "", "get");
        }
    }
}
