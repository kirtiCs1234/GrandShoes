@model Model.StoreDeliveryReportModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">

<div class="table-style">
    <div class="prtm-full-block pos-relative">
        <div class="prtm-block-title pad-all-md ">
            <div class="pos-relative">
                <div class="caption">
                    <h3 class="text-capitalize">Store Delivery  Report</h3>
                </div>
            </div>
        </div>
        <div class="modal-body">
            <div class="row  mrgn-b-lg">

                <div class="col-md-2 col-xs-12 col-sm-2">
                    <label>Branch </label>
                    @Html.DropDownListFor(m => m.BranchId, null, "Branch", htmlAttributes: new { @class = "form-control" })
                    @Html.ValidationMessageFor(m => m.BranchId, "", new { @class = "text-danger" })
                </div>
                <div class="col-md-3 col-xs-12 col-sm-3">
                    <label>From Date </label>
                    @Html.TextBoxFor(model => model.FromDate, " ", new { id = "toDate", @class = "form-control date-picker" })
                    @Html.ValidationMessageFor(m => m.FromDate, "", new { @class = "text-danger" })

                </div>
                <div class="col-md-3 col-xs-12 col-sm-3">
                    <label>To Date </label>
                    @Html.TextBoxFor(model => model.ToDate, " ", new { id = "fromDate", @class = "form-control date-picker" })
                    @Html.ValidationMessageFor(m => m.ToDate, "", new { @class = "text-danger" })

                </div>
                <div class="col-md-2 col-xs-12 col-sm-2">
                    <label> </label>
                    <button type="button" class="btn btn-primary export-btn" onclick="searchData()">Search</button>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="storeDelivery">
    @{
        Html.RenderPartial("_index1");
    }
</div>

@section scripts{
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script type="text/javascript">
        $(function () {
            $('.date-picker').datepicker({ dateFormat: 'dd-mm-yy' }).val();
        })

     
    </script>

    <script type="text/javascript">
        function searchData() {
            debugger;
            var dataObj = {};
            dataObj.BranchId = $('#BranchId').val();
            dataObj.ToDate = $('#toDate').val();
            dataObj.FromDate = $('#fromDate').val();
            $.ajax({
                url: "/admin/StoreDeliveryReport/_Index1",
                type: 'POST',
                data: { StoreModel: dataObj },
                success: function (result) {
                    debugger;
                    $('#storeDelivery').html(result);
                },
                error: function (xhr, status) {
                    alert(status)
                }
            });
            return false;
        }
        $(document).on("click", "a[href^='/Admin/StoreDeliveryReport/_Index1?page']", function () {
            $.ajax({
                type: "POST",
                url: $(this).attr("href"),
                // data: JSON.stringify(obj),
                success: function (result) {
                    $('#storeDelivery').html(result);
                },
                error: function (xhr, status) {
                    alert(status)
                }
            });
            return false;
        });
    </script>
}
