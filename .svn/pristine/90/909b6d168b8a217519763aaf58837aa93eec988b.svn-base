@model Model.CartonManagementReportModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<div class="table-style">
    <div class="prtm-full-block pos-relative">
        <div class="prtm-block-title pad-all-md ">
            <div class="pos-relative">
                <div class="caption">
                    <h3 class="text-capitalize">Carton Management Report</h3>
                </div>
            </div>
        </div>
        <div class="modal-body">
            <div class="row  mrgn-b-lg">
                <div class="col-md-4 col-xs-12 col-sm-4">
                    @Html.DropDownListFor(m => m.StockDistributionSummaryId, null, "Stock Distribution Summary ", htmlAttributes: new { @class = "form-control" })
                    @Html.ValidationMessageFor(m => m.StockDistributionSummaryId, "", new { @class = "text-danger" })
                </div>
                <div class="col-md-4 col-xs-12 col-sm-4">
                    @Html.DropDownListFor(m => m.BranchId, null, "Branch Name", htmlAttributes: new { @class = "form-control" })
                    @Html.ValidationMessageFor(m => m.BranchId, "", new { @class = "text-danger" })
                </div>
                <div class="col-md-2 col-xs-12 col-sm-2">
                    <button type="button" class="btn btn-primary export-btn" onclick="searchData()">Search</button>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="CartonmanagementReport">
    @{
        Html.RenderPartial("_index1");
    }
</div>

@section scripts{
    <script type="text/javascript">
        function searchData() {
            debugger;
            var cartonManagemant = {};

            cartonManagemant.BranchId = $('#BranchId').val();
            cartonManagemant.StockDistributionSummaryId = $('#StockDistributionSummaryId').val();
            $.ajax({
                url: "/admin/CartonManagementReport/_Index1",
                type: 'POST',
                data: { CartonManagement: cartonManagemant },
                success: function (result) {
                    debugger;
                    $('.CartonmanagementReport').html(result);
                },
                error: function (xhr, status) {
                    alert(status)
                }
            });
            return false;
        }
        $(document).on("click", "a[href^='/Admin/CartonManagementReport/_Index1?page']", function () {
            $.ajax({
                type: "POST",
                url: $(this).attr("href"),
                // data: JSON.stringify(obj),
                success: function (result) {
                    $('.CartonmanagementReport').html(result);
                },
                error: function (xhr, status) {
                    alert(status)
                }
            });
            return false;
        });
    </script>
}

