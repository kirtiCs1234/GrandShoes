@model List<Model.CartonManagementDetailModel>
<div class="table-style">
    <div class="row  mrgn-b-lg">
        <div class="col-sm-12 col-md-12 col-xs-12">
            <div class="forms">

                <div class="prtm-block-content mrgn-b-md">
                    <div class="table-responsive" style="height:270px;border: 1px solid #eeeeee;">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>ProductSKU </th>
                                    <th>Product Style</th>
                                    <th>Total</th>
                                    <th>Column No</th>
                                    <th>Grd No</th>
                                    <th>Carton No</th>
                                    <th>Packing Slip</th>

                                </tr>

                            </thead>
                            <tbody class="tbodyIndex" id="table2">
                                @foreach(var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ProductSKU)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.StyleSKU)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem=>item.Total)
                                        </td>
                                        <td></td>
                                        <td></td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CartonManagement.CartonNumber)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CartonManagement.DistributionSummaryID)
                                        </td>
                                    </tr>
                                    
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

