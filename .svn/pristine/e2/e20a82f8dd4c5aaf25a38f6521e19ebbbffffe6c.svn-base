    function CalculationCheck(ctrl) {
        debugger;
        // var initValue = $(ctrl).parents('td').find('.spanval').text();

        var inputValue = $(ctrl).val();
        var ctrlId = $(ctrl).attr("id");
        var itemNumber = ctrlId.substring(0, 10);
        var hiddenCtrl = $("#span_" + ctrlId + "");
        var spanV = hiddenCtrl.text();
        if (spanV == "") {
            spanV = 0;
        }
        var wareHouseCtrl = $("#quantitySize_" + ctrlId + "");
        var whareHouse = wareHouseCtrl.text();
        var temp = whareHouse;
        if (whareHouse == "")
            whareHouse = 0;
        if (inputValue == "") {
            inputValue = 0;
            whareHouse = temp;
        }
        var span = jQuery("#span_" + ctrlId + "").text(inputValue);
        var value = inputValue - spanV;
        if (parseInt(whareHouse) >= parseInt(value)) {
            whareHouse = parseInt(whareHouse) - parseInt(value);
            wareHouseCtrl.text(whareHouse);
        }
        else {
            alert("please enter Quantity of Stock Distrubution is less than Quantity of Stock Invebtory ");
            $(ctrl).val(0);
            hiddenCtrl.text(0);
            var ware = parseInt(spanV) + parseInt(whareHouse);
            wareHouseCtrl.text(ware);
        }


    }
