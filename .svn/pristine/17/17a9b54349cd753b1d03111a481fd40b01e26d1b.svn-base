                    debugger;
                    $('#demo').html(result);
                    var l = $("#table1 tr").length;
                    if (l == 0) {
                        $("#massage").text("Stock Distribution data not found this branch.");
                        $("#massage").css("color", "red");
                        document.getElementById("stock").disabled = true;
                    } 
                }
            })
        }
