@using Helper
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        @ViewBag.Title
    </title>
    <link href="/images/inusurance-favicon.ico" rel="shortcut icon" />
    <meta name="description" content="YOBI FUND is an online marketplace for buying and selling securities for start-up capital, real estate, and P2P lending. We host third party issuers as well as our own portfolio of subsidiary issuers across these vertical markets." />
    <link href="/Content/bootstrap.css" rel="stylesheet" />
    <link href="/Content/style.css" rel="stylesheet" />
    <link href="/Content/customsheet.css" rel="stylesheet" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css">
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/fancybox/3.0.47/jquery.fancybox.css">
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link href="/Content/toastr.css" rel="stylesheet" />
    <link href="/Content/multi-select.css" rel="stylesheet" />
    <link href="/Content/bootstrap-select.css" rel="stylesheet" />
    <script src='https://www.google.com/recaptcha/api.js'></script>
    <link href="/Content/bootstrap-colorpicker/css/bootstrap-colorpicker.css" rel="stylesheet" />

    @*<--Signup-->*@
    <link rel="shortcut icon" type="image/png" href="/Content/assets/img/favicon.png">
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet">
    @*<link rel="stylesheet" href="/Content/assets/plugins/animate/animate.min.css" type="text/css" />*@
    <link rel="stylesheet" type="text/css" href="/Content/assets/css/pratham.min.css">
    <link rel="stylesheet" type="text/css" href="/Content/assets/css/vendor.min.css">
    <link href="https://code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css" rel="stylesheet" type="text/css" />
    <link href="/Content/assets/css/MonthPicker.min.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" type="text/css" href="/Content/assets/css/examples.css" />

</head>
<body>
    <div class="loader_bg">
        <div class="loader">
            <div class="item item-1"></div>
            <div class="item item-2"></div>
            <div class="item item-3"></div>
            <div class="item item-4"></div>
        </div>
    </div>
    <div id="wrapper">

        
        @RenderBody()
    </div>
    <script src="/Scripts/jquery-1.12.4.js"></script>
    <script src="/Scripts/jquery.validate.min.js"></script>
    <script src="/Scripts/jquery.validate.unobtrusive.min.js"></script>
    @*<script src="/Scripts/jquery-1.10.2.js"></script>*@
    <script src="/Scripts/jquery-ui-1.12.1.js"></script>

    <script src="/Scripts/toastr.js"></script>
    <script src="/Scripts/Common.js"></script>
    <script src="/Scripts/bootstrap-select.js"></script>
    <script src="/Scripts/dpEncodeRequest.js"></script>
    <script src="https://code.highcharts.com/highcharts.js"></script>
    <script src="https://code.highcharts.com/highcharts-3d.js"></script>
    <script src="/Scripts/dpEncodeRequest.js"></script>
    @* <script src="/Scripts/carquery.js"></script>*@
    <script src="/Scripts/bootstrap.min.js"></script>
    <link href="https://eonasdan.github.io/bootstrap-datetimepicker/css/base.css" rel="stylesheet">
    <link href="https://cdn.rawgit.com/Eonasdan/bootstrap-datetimepicker/e8bddc60e73c1ec2475f827be36e1957af72e2ea/build/css/bootstrap-datetimepicker.css" rel="stylesheet">
    <script src="/Scripts/multi-select.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.9.0/moment-with-locales.js"></script>
    <script src="https://cdn.rawgit.com/Eonasdan/bootstrap-datetimepicker/e8bddc60e73c1ec2475f827be36e1957af72e2ea/src/js/bootstrap-datetimepicker.js"></script>
    <script src="/Scripts/wow.js"></script>
    <script src="/Scripts/multi-select.js"></script>
    <script src="/Scripts/login-page_script.js"></script>
    <script src="/Scripts/bootstrap-colorpicker.js"></script>
    <script src="/Scripts/lightslider.js"></script>
    <script src="/Scripts/handlebars.js"></script>

    @* Signup *@
    <script src="/Scripts/assets/js/main.js" type="text/javascript"></script>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script src="/Scripts/assets/js/vendor.min.js" type="text/javascript"></script>
    <script src="/Scripts/assets/js/pratham.min.js" type="text/javascript"></script>
    <script src="/Scripts/assets/js/MonthPicker.min.js"></script>
    <script src="/Scripts/assets/js/examples.js"></script>

    <script>
        wow = new WOW(
          {
              animateClass: 'animated',
              offset: 100
          }
        );
        wow.init();
    </script>

    @RenderSection("scripts", required: false)

    <script>
        function openNav() {
            document.getElementById("mySidenav").style.width = "250px";
        }

        function closeNav() {
            document.getElementById("mySidenav").style.width = "0";
        }
    </script>


    <script>
        $("body").click(function () {

            if ($("#mySidenav").width() > 0) {

                document.getElementById("mySidenav").style.width = "0";
            }
        });

    </script>


    <div class="container-fluid bottom-price-container compare-div" style="display:none">
        <div class="container">
            <div class="row text center">
                <div class="col-md-8 col-xs-12 compare-list">

                </div>
                <div class="col-md-4 col-xs-12">
                    <div class="col-md-12 compare-plan col-xs-12">
                        <span class="compare-btn">Compare Plan</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--Start of Tawk.to Script-->
    <script type="text/javascript">
        var Tawk_API = Tawk_API || {}, Tawk_LoadStart = new Date();
        (function () {
            var s1 = document.createElement("script"), s0 = document.getElementsByTagName("script")[0];
            s1.async = true;
            s1.src = 'https://embed.tawk.to/59550ecee9c6d324a4737f63/default';
            s1.charset = 'UTF-8';
            s1.setAttribute('crossorigin', '*');
            s0.parentNode.insertBefore(s1, s0);
        })();
    </script>
    <!--End of Tawk.to Script-->
</body>
</html>