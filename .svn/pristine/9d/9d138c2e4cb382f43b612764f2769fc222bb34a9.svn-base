    <script type="text/javascript">
       
    $('#myTable').append('<tbody><tr id="AddNewItem"><td colspan="4" class="text-right"><i class="fa fa-plus-circle" onclick="AddRow(this)" aria-hidden="true"></i></td></tr>  ');
    function AddReceiptOrder() {
        debugger;
        var obj = {};
        obj.ReceiptOrderId = $('#ReceiptOrderId').val();
        obj.autoCompleteProductName = $("autoCompleteProductName").val();
        obj.autoCompleteProductStyleName = $("autoCompleteProductStyleName").val();
        if (obj.autoCompleteProductName == null && obj.autoCompleteProductStyleName == null) {
            document.getElementById('lable').innerHTML = 'Please add products quantity.';
        } else if (obj.autoCompleteProductName == null && obj.autoCompleteProductStyleName == null) {

            $.ajax(
                  {
                      type: "GET", //HTTP POST Method
                      url: '/Admin/Receipt/AddReceiptOrder',
                      data: obj,
                      success: function (result) {
                          window.location.href = "/Admin/Receipt/Index";
                      },
                      error: function (errormessage) {
                          alert(errormessage.responseText);
                      }
                  });
        }
    }
    function AddRow(id) {
        debugger;
        var i = id.parentNode.parentNode.rowIndex-2;
