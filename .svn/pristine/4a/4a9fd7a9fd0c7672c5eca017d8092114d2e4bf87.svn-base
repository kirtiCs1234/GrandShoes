using Services.IService;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using Model.ProductStyle;
using Helper;

namespace Services.Service
{
    public class ProductStyleService : IProductStyleService
    {
        public List<ProductStyle> ProductStyleAutocomplete(string name)
        {
            return ServerResponse.Invoke<List<ProductStyle>>("api/ProductStyle/ProductStyleAutocomplete?name=" + name, "", "get");
        }
    }
}
