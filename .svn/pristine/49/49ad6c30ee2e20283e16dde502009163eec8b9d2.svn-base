<div class="col-md-12">
    @if (ViewBag.endPage > 1)
    {
        <div class="pagination">

            @if (ViewBag.CurrentPage > 1)
            {
                <li>
                    <a href="/Admin/StoreDeliveryReport/_Index1?page=1">First</a>
                </li>
                <li>
                    <a href="/Admin/StoreDeliveryReport/_Index1?page=@(ViewBag.CurrentPage - 1)">Previous</a>
                </li>
            }
            @foreach (var i in ViewBag.result)
            {
                var page = i;
                <li class="@(page == ViewBag.CurrentPage ? "active" : "")">
                    <a href="/Admin/StoreDeliveryReport/_Index1?page=@page">@page</a>
                </li>
            }
            @if (ViewBag.CurrentPage < ViewBag.pageCount)
            {
                <li>
                    <a href="/Admin/StoreDeliveryReport/_Index1?page=@(ViewBag.CurrentPage + 1)">Next</a>
                </li>
                <li>
                    <a href="/Admin/StoreDeliveryReport/_Index1?page=@(ViewBag.pageCount)">Last</a>
                </li>
            }
        </div>
    }
</div>