<script  type="text/javascript">

    function searchData(dec) {
    debugger;
        obj = {};
        obj.BramchFrom = $('.branch-from').val();
        obj.BramchTo = $('.branch-to').val();
        obj.ProductSKUFrom = $('.productsku-from').val();
        obj.ProductSKUTo = $('.productsku-to').val();
        obj.StyleSKUFrom = $('.stylesku-from').val();
        obj.StyleSKUTo = $('.stylesku-to').val();
        obj.AllwithMarkdown = $('.all').val();
        obj.OnlyMarkdown = $('.markdown').val();
        obj.NoMarkdown = $('.nomarkdown').val();
    $.ajax({
                type: "POST",
                url: '/Admin/Report/_BranchSTockStatusReport',

                data: obj,
                success: function (result) {
                    $('.dvUserDetails').html(result);
                    //  alert('hdfjdhfj');
                },
                error: function (xhr, status) {
                    alert(status)
                }

            });
    }

</script>