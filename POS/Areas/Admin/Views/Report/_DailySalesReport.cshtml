@model List<Model.Report.DailySellReport>
<div class="prtm-full-block pos-relative">
    <div class="prtm-block-title pad-all-md">
        <div class="pos-relative">
            <div class="caption">
            </div>
        </div>
    </div>
    <div class="prtm-block-content mrgn-b-md">
        <div class="table-responsive">
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>Product</th>
                        <th>Sales Quantity</th>
                        <th>Sales Total Price</th>
                        <th>LayBaySale Quantity</th>
                        <th>LayBaySale Price</th>
                        <th>Total Quantity</th>
                        <th>Total Price</th>
                    </tr>
                </thead>
                <tbody class="tbodyIndex">
                    @if (Model.Count != 0)
                    {
                        foreach (var item in Model)
                        {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Product)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SalesQuantity)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SalesPrice)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.LayBayQuantity)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.LayBayPrice)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TotalQuantity)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TotalPrice)
                        </td>
                    </tr>
                        }
                    }
                    else
                    {
                        <tr>
                            <td colspan="7">
                                <p>No record found.</p>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>



