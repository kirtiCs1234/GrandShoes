                var totalCtrl1 = $(".tbodyIndex")[1].children[i].children[2].getAttribute("id");
                var totalCtrl = $("#" + totalCtrl1);
                var total = $(".tbodyIndex")[1].children[i].children[2].innerText;
                var totalCartonCtrl = $("#Total_" + k);
                var hiddenCtrl = $("#span_" + ctrlId + "");
                var spanV = hiddenCtrl.text();
                if (spanV == "") {
                    spanV = 0;
                }
                var totalCarton = totalCartonCtrl.val();
                if (totalCarton == "") {
                    totalCarton = 0;
                }
                if (isNaN(totalCarton)) {
                    totalCarton = 0;
                }
                var temp = whareHouse;
                if (whareHouse == "")
                    whareHouse = 0;
                if (inputValue == "") {
                    inputValue = 0;
                    whareHouse = temp;
                }
                var value = inputValue - spanV;
                var cartonTotal = parseInt(totalCarton) + parseInt(value);
                if (cartonTotal > 24) {
                    alert("please store only 24 items in one carton.");
                    if (spanV == "") {
                        $(ctrl).val(0);
                    }
                    else {
                        $(ctrl).val(spanV);
                    }
                }

                else {
                    var span = jQuery("#span_" + ctrlId + "").text(inputValue);

                    if (parseInt(whareHouse) >= parseInt(inputValue)) {
                        whareHouse = parseInt(whareHouse) - parseInt(value);
                        total = parseInt(total) - parseInt(value);
                        wareHouseCtrl.text(whareHouse);
                        totalCtrl.text(total);
                        totalCartonCtrl.val(cartonTotal);

                    }
                    else {
                        alert("please enter Quantity of Stock Distrubution is less than Quantity of Stock Invebtory ");
                        $(ctrl).val(0);
                        $("#span_" + itemNumber + "").text(0);
                    }

                }
                return 0;

            }

            i++;

        }
    }
