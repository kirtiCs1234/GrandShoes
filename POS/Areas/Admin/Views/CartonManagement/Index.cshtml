@model Model.CartonManagementStockModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
<div class="row">
    <div class="prtm-page-bar col-md-5 col-lg-3 col-sm-12 col-12" style="background:none;">
        <ul class="breadcrumb" style="background:none;">
            <li class="breadcrumb-item text-capitalize">
                <h3>Carton</h3>
            </li>
            <li class="breadcrumb-item"><a href="#">Dashboard</a></li>
        </ul>
    </div>
    <div class="col-md-8 col-lg-6 col-sm-8 col-12 search-container">
        <div class="top-search-navbar-search smallsearch col-md-12 col-lg-12 col-sm-12 col-12">
            <div class="row">

                <input class="top-search-navbar-input col-md-8 col-lg-10 col-sm-8 col-xs-8 searchiFirstName" type="text" placeholder="Search Column Number" Id="CNumber" name="iFirstName">

                @*<input type="submit" value="Add" class="btn btn-success popup-savebtn" />*@

                <button class="advance-search col-md-2 col-lg-1 col-sm-2 col-xs-2" type="button">
                    
                </button>

                <button class="top-search-navbar-button col-md-2 col-lg-1 col-sm-2 col-xs-2" onclick="searchData1()">
                    <i class="fa fa-search" aria-hidden="true"></i>
                </button>
            </div>
        </div>

    </div>
    <div class="add-btn-div col-md-4 col-lg-3 col-sm-4 col-12 text-right">
        <button type="button" class="btn btn-primary import-btn">Import</button>&nbsp;&nbsp;
        <button type="button" class="btn btn-primary export-btn">Export</button>&nbsp;&nbsp;
    </div>
</div>
<div class="table-style">
    @if (TempData["Success"] != null)
    {
        <p class="alert alert-success" id="successMessage" style="width: 300px;height: 25px;color: #fff;text-align: center; padding-bottom: 2em;">
            @TempData["Success"]
        </p>
    }
    <script>
        setTimeout(function () {
            $('#successMessage').fadeOut('slow');
        }, 5000);
    </script>
    <div class="dvUserDetails">
        @{

            Html.RenderPartial("_Index1", Model);
        }

    </div>

</div>
<div class="modal fade" id="myModal">
    <div id="dvCreatedetails">

    </div>

</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
@section scripts{


    <script src="//code.jquery.com/ui/1.11.2/jquery-ui.js"> </script>
    <script type="text/javascript">
        $(document).on("click", "#cancelyesbtn", function () {
            window.location = '/admin/area/index';
        });
        function searchData1() {
            debugger;
            var obj = {};

            obj.Id = $('#CNumber').val();
            obj.BranchName = $('#CNumber').val();
            obj.IsActive = true;
            $.ajax({
                url: '/Admin/CartonManagement/_Index1',
                type: 'POST',
                data: obj,
                success: function (result) {
                    $('.dvUserDetails').html(result);
                },
                error: function (xhr, status) {
                    alert(status)
                }

            });
            return false;
        }

        $(document).on("click", "a[href^='/Admin/CartonManagement/_Index1?page']", function () {
            debugger
            var obj = {};

            obj.Id = $('#CNumber').val();
            obj.BranchName = $('#CNumber').val();
            obj.IsActive = true;
            $.ajax({
                type: "POST",
                url: $(this).attr("href"),
                // data: JSON.stringify(obj),
                data: obj,
                success: function (result) {
                    $('.dvUserDetails').html(result);
                    //  alert('hdfjdhfj');
                },
                error: function (xhr, status) {
                    alert(status)
                }

            });
            return false;
        });
        function showReceipt(id) {
            debugger
          //  alert(id);
           $("#ReceiptDetail-" + id).html('<tr><td colspan = "5" style="background-color:white;"><img src="/Image/bars.gif" style="width:55px; height:47px;margin-left:40%; z-index:200" /></td></tr>');

            $.ajax(
             {
                 url: '/Admin/CartonManagement/GetCartonDetailByCartonId/' + id,
                 type: "GET",
                success: function (result) {
                    debugger
                    $("#CartonDetail-" + id).html(result);
                   // alert("dgfhdf");
           },
           error: function (xhr, status) {
             alert(status)
           }
           });
        }
        $(".accordian-btn").click(function () {
            $(this).toggleClass("accordian-reverse");
            $(this).parent().parent().next().find(".accordian-bundling").toggle(500);
        });
        var id1;
        function del(del){
            debugger
            id1=del;
            Data(0);
        }
        
        function Data(ch) {
           debugger
            var crud = "";
            switch (ch) {
                
                case 0:
                    crud='/Admin/CartonManagement/Delete/'+id1;
                    break;
              
            }
            $.ajax({
                url: crud,
                //data: obj,
                success: function (result) {
                    debugger;
                    // alert('ssss');
                    $('#dvCreatedetails').html(result);
                    //loadData();
                },
                error: function (xhr, status) {
                    alert(status)
                }

            });
            return false;

        }
        var id2;
        function edit(edit){
            debugger
            id2=edit;
            var obj={};

            obj.BranchId=$(".tbodyIndex")[0].children[0].cells[0].innerText,
            
            $.ajax({
                url: '/Admin/CartonManagement/Edit/'+id2,
                type: 'GET',
                data: obj,
                success: function (result) {
                    $('#dvCreatedetails').html(result);
                },
                error: function (xhr, status) {
                    alert(status)
                }

            })
            return false;
        }
        
        function searchData() {
            debugger;
            var obj = {};
            obj.StockDistributionSummaryId = $('#StockDistributionSummaryId').val();

            obj.BranchId = $('#BranchId').val();
            $.ajax({
                url: "/Admin/CartonManagement/Create",
                type: 'POST',
                data: obj,
                success: function (result) {
                    $('#dvCreatedetails').html(result);
                },
                error: function (xhr, status) {
                    alert(status)
                }

            })
            return false;
        }
    </script>
}