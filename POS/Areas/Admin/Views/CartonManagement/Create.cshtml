    function CalculationCheck(ctrl) {
        debugger;
        
        var inputValue = $(ctrl).val();
        var ctrlId = $(ctrl).attr("id");
        var c = $(ctrl).closest("td").index();
        var k=ctrlId.substring(11);
        var itemNumber = ctrlId.split("_")[0];
        var ProductId = $("#productSelect_"+k+"").attr("id");
        var ProductIdValue = $("#productSelect_"+k+"").val();
        //  var ProductIdValue = ProductIdValue1;
        var childId = $("#table").attr("id");
        var itemNumber = ctrlId.substring(0, 10);
        var hiddenCtrl = $("#span_" + ctrlId + "");
        var spanV = hiddenCtrl.text();
        var count = $('#table').children().length;
        var i = 0;
        while (i < count) {
            debugger
            var ProductId2Value = $("#item_Id_" + i).val();
            if (ProductId2Value == ProductIdValue) {
               debugger
                // var wareHouseCtrl = $("#item_" + itemNumber + "_" + i);
                //  var whareHouse = wareHouseCtrl.val();
                var wareHouseCtrl1 = $(".tbodyIndex")[1].children[i].cells[c].getAttribute("id");
                var wareHouseCtrl = $("#" + wareHouseCtrl1);
                var whareHouse = $(".tbodyIndex")[1].children[i].cells[c].innerText;
                 var totalCtrl = $("#Total_" + k);
                 var total = totalCtrl.val();
            //  var wharehouse=$("#item_"+itemNumber +"_" + i).val();
              var totalCtrl1 = $(".tbodyIndex")[1].children[i].children[2].getAttribute("id");
                var totalCtrl = $("#" + totalCtrl1);
               var total = $(".tbodyIndex")[1].children[i].children[2].innerText;
              var totalCartonCtrl = $("#Total_"+k);
              var hiddenCtrl = $("#span_" + ctrlId + "");
                var spanV = hiddenCtrl.text();
                if (spanV == "") {
                    spanV = 0;
                }
                var totalCarton = totalCartonCtrl.val();
                if (totalCarton == "") {
                    totalCarton = 0;
                }
                if (isNaN(totalCarton)) {
                    totalCarton = 0;
                }
                var temp = whareHouse;
                if (whareHouse == "")
                    whareHouse = 0;
                if (inputValue == "") {
                    inputValue = 0;
                    whareHouse = temp;
                }
                var value = inputValue - spanV;
                var cartonTotal = parseInt(totalCarton) + parseInt(value);
                if (cartonTotal > 24) {
                    alert("please store only 24 items in one carton.");
                    if (spanV == "") {
                        $(ctrl).val(0);
                    }
                    else {
                        $(ctrl).val(spanV);
                    }
                }

                else {
                    var span = jQuery("#span_" + ctrlId + "").text(inputValue);

                    if (parseInt(whareHouse) >= parseInt(inputValue)) {
                        whareHouse = parseInt(whareHouse) - parseInt(value);
                        total = parseInt(total) - parseInt(value);
                        wareHouseCtrl.text(whareHouse);
                        totalCtrl.text(total);
                        totalCartonCtrl.val(cartonTotal);

                    }
                    else {
                        alert("please enter Quantity of Stock Distrubution is less than Quantity of Stock Invebtory ");
                        $(ctrl).val(0);
                        $("#span_" + itemNumber + "").text(0);
                    }

                }
                return 0;

            }

            i++;

        }
    }
                     function del(id) {
        debugger;
        var Id = id.parentNode.parentNode.rowIndex;
        var quantity = parseFloat($(id).parents("tr").find("[id^='Quantity']").val());
        if (isNaN(quantity)) { quantity = 0; }
        var unitCost = parseFloat($(id).parents("tr").find("[id^='UnitCost']").val());
        if (isNaN(unitCost)) { unitCost = 0; }
        var price = parseFloat($(".ItemAmount").val()).toFixed(2);
        if (isNaN(price)) { price = 0; }
        price -= quantity * unitCost;
        $(id).parents("tr").remove();
        $(".ItemAmount").val(price);

    };
