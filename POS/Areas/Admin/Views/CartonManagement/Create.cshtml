                <script type="text/javascript">
    $('#myTable').append('<tbody><tr id="AddNewItem"><td colspan="4" class="text-right"><i class="fa fa-plus-circle" onclick="AddRow(this)" aria-hidden="true"></i></td></tr>  ');
    function AddCartonManagement() {
        debugger;
        var obj = {};
        obj.CartonManagementID = @ViewBag.CartonManagementID;
        $.ajax(
              {
                  type: "GET", //HTTP POST Method
                  url: '/Admin/CartonManagement/AddCarton',
                  data: obj,
                  success: function (result) {
                      window.location.href = "/Admin/CartonManagement/Index";
                  },
                  error: function (errormessage) {
                      alert(errormessage.responseText);
                  }
              });
    }
    function AddRow(id) {
        debugger;
